<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Token.SDK.Net</AssemblyName>
    <RootNamespace>Tokenio</RootNamespace>
    <Copyright>Copyright 2019 Token, Inc</Copyright>
    <Description>C# SDK for interacting with the Token System</Description>
    <Company>Token, Inc</Company>
    <Authors>Token, Inc</Authors>
    <Version>2.0.0-beta3</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIconUrl>https://s3-us-west-2.amazonaws.com/tokenio-assets/1.0.0/logos/token_black.png</PackageIconUrl>
